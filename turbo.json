{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "lib/**",
        "*.js"
      ]
    },
    "lint": {},
    "clean": {
      "cache": false
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}
